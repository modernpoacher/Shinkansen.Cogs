{
  "name": "shinkansen-cogs",
  "version": "1.0.1",
  "description": "Shinkansen Cogs",
  "main": "./index.js",
  "keywords": [
    "Shinkansen",
    "Cogs",
    "Zashiki Karakuri",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-cogs.git"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run lint:fix && npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src stories .storybook *.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose false --collect-coverage false",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm run deps-install:peer && npm test && npm run babel && git add lib",
    "deps": "deps",
    "deps:prod": "deps --save-prod",
    "deps:dev": "deps --save-dev",
    "deps-install:peer": "deps-install --peer"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@modernpoacher/deps": "0.0.12",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
